{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "hg": {
        "ClusterId": "hg-web",
        "Authorization Policy": "HgAuth",
        "Match": {
          "Path": "/hg/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "hg-web": {
        "Destinations": {
          "hg-web-server": {
            "Address": "http://hg-web/"
          }
        }
      }
    }
  }
}
