{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Trace"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "hg": {
        "ClusterId": "hg-web",
        "AuthorizationPolicy": "HgAuth",
        "Match": {
          "Path": "/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "hg-web": {
        "Destinations": {
          "hg-web-server": {
            "Address": "https://hg-public.languageforge.org/"
          }
        }
      }
    }
  },
  "LexBoxApi": {
    "Url": null
  }
}
