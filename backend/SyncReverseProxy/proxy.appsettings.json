{
  "ReverseProxy": {
    //config reference https://microsoft.github.io/reverse-proxy/articles/config-files.html#configuration-structure
    "Routes": {
      "resumable": {
        "Order": 10,
        "ClusterId": "resumableWeb",
        "AuthorizationPolicy": "UserHasAccessToProject",
        "Match": {
          "Path": "/api/v03/{**catch-all}"
        }
      },
      "hg": {
        "Order": 20,
        "ClusterId": "hgWeb",
        "AuthorizationPolicy": "UserHasAccessToProject",
        "Match": {
          "Path": "/{project-code}"
        },
        "Transforms": [
          {
            "PathPrefix": "/hg"
          }
        ]
      },
      "hgWebView": {
        "Order": 11,
        "ClusterId": "hgWeb",
        "AuthorizationPolicy": "UserHasAccessToProject",
        "Match": {
          "Path": "/api/hg-view/{project-code}/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api/hg-view"
          },
          {
            "PathPrefix": "/hg"
          }
        ]
      },
      "hgWebViewStaticRewrite": {
        "Order": 10,
        "ClusterId": "hgWeb",
        "Match": {
          "Path": "/hg/static/{**catch-all}",
          "Headers":[
            {
              "Name": "Referer",
              "Values": [
                "/api/hg-view/",
                "/static/hgk/css/icofont.min.css"
              ],
              "Mode": "Contains"
            }
          ]
        }
      }
    },
    "Clusters": {
      "hgWeb": {
        "Destinations": {
          "hgWebServer": {
            "Address": "http://localhost:8088/"
          }
        }
      },
      "resumableWeb": {
        "Destinations": {
          "resumableWebServer": {
            "Address": "http://localhost:8034/"
          }
        }
      }
    }
  },
  "LexBoxApi": {
    "Url": null
  }
}
